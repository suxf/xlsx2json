{
  "xlsx": {
    "src": "../excel/**/[^~$]*.xls[xm]",
    "json_server_dest": "./bin/json_server",
    "json_client_dest": "./bin/json_client",
    "ignore_prefix_symbol": "~",
    "only_export_server_prefix": "_"
  },
  "ts": false,
  "json": {
    "extension": ".json",
    "client_uglify": true,
    "server_uglify": false
  },
  "cs": true,
  "csharp": {
    "namespace": "Xlsx2Json",
    "manager_name": "Configs",
    "row_suffix": "Cfg",
    "collection_suffix": "Cfgs",
    "has_extension": false,
    "csharp_server_dest": "./bin/csharp_server",
    "csharp_client_dest": "./bin/csharp_client"
  }
}